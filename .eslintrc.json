{
  "extends": ["standard", "standard-react", "prettier"],
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "react/prop-types": [1, { "skipUndeclared": true }],
    "curly": [1, "multi"],
    "operator-linebreak": [
      1,
      "before",
      { "overrides": { "?": "ignore", ":": "ignore" } }
    ]
  }
}
